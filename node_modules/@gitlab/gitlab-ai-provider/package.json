{
  "name": "@gitlab/gitlab-ai-provider",
  "version": "3.3.1",
  "description": "GitLab Duo provider for Vercel AI SDK",
  "license": "MIT",
  "author": {
    "name": "Vladimir Glafirov",
    "email": "vglafirov@gitlab.com",
    "url": "https://gitlab.com/vglafirov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/gitlab-org/editor-extensions/gitlab-ai-provider.git"
  },
  "bugs": {
    "url": "https://gitlab.com/gitlab-org/editor-extensions/gitlab-ai-provider/-/issues"
  },
  "homepage": "https://gitlab.com/gitlab-org/editor-extensions/gitlab-ai-provider#readme",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "rebuild": "npm run clean && npm run build",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "lint-staged": "lint-staged",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "semantic-release": "semantic-release"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@anycable/core": "^0.9.2",
    "graphql-request": "^6.1.0",
    "isomorphic-ws": "^5.0.0",
    "openai": "^6.16.0",
    "socket.io-client": "^4.8.1",
    "vscode-jsonrpc": "^8.2.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "@ai-sdk/provider-utils": "^3.0.17",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^13.0.3",
    "@semantic-release/npm": "^13.1.3",
    "@types/node": "^20.17.24",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "semantic-release": "^25.0.2",
    "tsup": "^8.0.0",
    "typescript": "^5.8.3",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@ai-sdk/provider": ">=2.0.0",
    "@ai-sdk/provider-utils": ">=3.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "ai",
    "gitlab",
    "gitlab-duo",
    "duo",
    "language-model",
    "llm",
    "vercel-ai-sdk",
    "ai-sdk",
    "chat",
    "chatbot",
    "agentic",
    "tool-calling",
    "anthropic",
    "claude",
    "code-generation",
    "code-completion",
    "developer-tools",
    "devops",
    "typescript",
    "ml",
    "machine-learning"
  ]
}
